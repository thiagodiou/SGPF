						<f:convertNumber groupingUsed="true" minFractionDigits="2" />
					</h:outputText>
				</rich:column>
				
				<rich:column style="width: 5%; text-align: center;">				
					<h:outputText value="#{liqc.quantidadeLiquidacoesTotal}" />
				</rich:column>
				
				<rich:column style="width: 5%; text-align: center;">
					<h:outputText value="#{liqc.valorLiquidacoesTotal}">
						<f:convertNumber groupingUsed="true" minFractionDigits="2" />
					</h:outputText>
				</rich:column>
									
			</rich:dataTable>
		</div>
		
